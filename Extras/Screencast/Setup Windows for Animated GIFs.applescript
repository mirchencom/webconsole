tell application "Finder"	set theScriptContainer to container of (path to me)	set theSetupWindowsFile to file "Setup Windows.scpt" of theScriptContainerend tellset theSetupWindowsScript to load script (theSetupWindowsFile as alias)set theWindows to theSetupWindowsScript's windowsForApplications({"TextMate", "Web Console"})theSetupWindowsScript's setupWindows(theWindows, {1132, 200})